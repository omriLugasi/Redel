{
  "name": "redel",
  "version": "0.6.0-beta.1",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "Promise based HTTP client for the browser and node.js",
  "scripts": {
    "prebuild": "rm -rf ./dist && mkdir dist",
    "build": "minify src/index.js --out-file dist/input.min.js --mangle.keepClassName",
    "prepublish": "npm run test && npm run eslint:src && npm run eslint:test && npm run build",
    "test": "ALLOW_LOG=false mocha --recursive ./test",
    "covarge:html": "nyc --r=html mocha --recursive ./test",
    "covarge": "nyc mocha --recursive ./test",
    "eslint:src": "eslint src/",
    "eslint:test": "eslint test/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omriLugasi/Redel"
  },
  "keywords": [
    "xhr",
    "http",
    "ajax",
    "promise",
    "node",
    "axios",
    "middleware",
    "middlewares",
    "plugins",
    "interceptors",
    "axios instance",
    "cancel",
    "cancelToken",
    "pending",
    "statistics",
    "XHR logs"
  ],
  "author": "Omri Luggasi",
  "dependencies": {
    "axios": "0.19.0",
    "qs": "6.9.0",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "babel-minify": "^0.5.1",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "express": "^4.17.1",
    "husky": "^3.0.7",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test && yarn eslint:src && yarn eslint:test"
    }
  },
  "nyc": {
    "exclude": [
      "server",
      "test",
      "src/services/logger"
    ]
  }
}
